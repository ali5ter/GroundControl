<ZAxisPopupContent>:
    distBtn:distBtn
    unitsBtn:unitsBtn
    cols: 1
    size: root.size
    pos: root.pos
    GridLayout:
        cols: 3
        Button:
            text: ".1"
            id: distBtn
            on_release: root.setDist()
        Button:
            text: "MM"
            on_release: root.units()
            id: unitsBtn
        Button:
            text: "Z-In"
            on_release: root.zIn()
        Button:
            text: "Done"
